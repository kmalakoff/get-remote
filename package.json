    "temp-suffix": "^1.0.10"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.1",
    "cr": "^0.1.0",
    "fast-extract": "^1.8.15",
    "fs-iterator": "^6.1.11",
